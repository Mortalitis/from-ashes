    <selectionEntry type="upgrade" import="true" name="HK Module Upgrade" hidden="false" id="dc06-4d3f-f92e-6e2c">
      <rules>
        <rule name="Hunter Killer" id="3208-42ba-92d4-7a90" hidden="false">
          <description>After this Squad finishes a Command and after any Reactions, it may attempt a Discipline test. If passed, it can immediately carry out an Engage Command, but only with a single non-Barrage weapon. This Command comes from the Orders allocation rather than its Commands</description>
        </rule>
        <rule name="Rangefinder" id="c513-537b-4324-efda" hidden="false">
          <description>After choosing a target, roll a D12; on a 7+ (5+ if not moved this turn) receive [Accuracy ← 1] for this Engage Command.</description>
        </rule>
      </rules>
    </selectionEntry>
    <selectionEntry type="upgrade" import="true" name="Snapdragon Auto Tracker" hidden="false" id="8058-bb62-4244-b40e">
      <rules>
        <rule name="Auto Tracker" id="7355-d205-49cc-0011" hidden="false">
          <description>A Snapdragon fitted with an Auto Tracker provides the Dual Autocannons with the Coordinated Defence Network and Free Command: Auto Fire special rules</description>
        </rule>
        <rule name="Coordinated Defence Network" id="8f82-fb59-9596-5329" hidden="false">
          <description>If this Squad is Ordered to Engage by a friendly Raven, the Dual Autocannons may reroll all failed Accuracy rerolls</description>
        </rule>
      </rules>
      <infoLinks>
        <infoLink name="Free Command: Autofire" id="2ffe-e098-1a7b-c447" hidden="false" type="rule" targetId="e35f-a703-f839-077c"/>
      </infoLinks>
    </selectionEntry>
    <selectionEntry type="upgrade" import="true" name="Counter Battery Radar" hidden="false" id="b944-f28a-0ef7-fb8b">
      <rules>
        <rule name="Counter Battery Radar" id="dd8a-b8fe-85f5-dc8f" hidden="false">
          <description>An Aurora fitted with this Upgrade receives the Acoustic Radar and Counter Battery abilities</description>
        </rule>
        <rule name="Acoustic Radar" id="7de6-8980-a680-00f8" hidden="false">
          <description>When this Unit fires a Barrage weapon and is targeting an enemy which itself has Engaged or Stormed one or more times this turn, it can reroll failed Static Targets attempts</description>
        </rule>
      </rules>
    </selectionEntry>
